{
    "name": "florianwolters/component-util-singleton",
    "description": "The Singleton (and Registry of Singletons a.k.a. Multiton) design pattern as a PHP component.",
    "type": "library",
    "keywords": ["singleton", "utility", "pattern", "creation"],
    "homepage": "http://github.com/FlorianWolters/PHP-Component-Util-Singleton",
    "license": "LGPL-3.0+",
    "authors": [
      {
        "name": "Florian Wolters",
        "email": "wolters.fl@gmail.com",
        "homepage": "http://blog.florianwolters.de",
        "role": "Developer"
      }
    ],
    "require": {
        "php": ">=5.4",
        "florianwolters/component-util-reflection": "0.2.*",
        "florianwolters/component-core-stringutils": "0.3.*"
    },
    "require-dev": {
        "pdepend/pdepend": "2.*",
        "phpmd/phpmd": "2.*",
        "phpunit/phpunit": "4.*",
        "sami/sami": "2.*",
        "sebastian/phpcpd": "2.*",
        "sebastian/phpdcd": "1.*",
        "satooshi/php-coveralls": "0.*@dev",
        "sensiolabs/security-checker": "2.*",
        "squizlabs/php_codesniffer": "2.*@dev"
    },
    "autoload": {
        "psr-4": {
            "FlorianWolters\\Component\\Util\\Singleton\\": "src/main/php"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "FlorianWolters\\Component\\Util\\Singleton\\": "src/test/php",
            "FlorianWolters\\Example\\": "src/test/resources"
        }
    }
}
